{
    "name": "kluseg/laravel-bootstrap-select",
    "description": "Set of selects utilizing bootstrap's 4 dropdowns paired with Laravel components",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Kluska",
            "email": "a.kluska98@gmail.com",
            "homepage": "https://github.com/kluseg/bootstrapselect"
        }
    ],
    "homepage": "https://github.com/kluseg/bootstrapselect",
    "keywords": ["Laravel", "BootstrapSelect"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KluseG\\BootstrapSelect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KluseG\\BootstrapSelect\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KluseG\\BootstrapSelect\\BootstrapSelectServiceProvider"
            ],
            "aliases": {
                "BootstrapSelect": "KluseG\\BootstrapSelect\\Facades\\BootstrapSelect"
            }
        }
    }
}
